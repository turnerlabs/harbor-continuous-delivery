{
  "name": "harbor-continuous-delivery",
  "version": "0.0.4",
  "description": "continously deploys harbor builds",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.15.1",
    "browser-request": "^0.3.3",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "harbor-client": "git+https://github.com/turnerlabs/harbor-client.git",
    "material-ui": "^0.15.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "build": "browserify src/index.jsx -t babelify -o public/bundle.js",
    "watch": "watchify src/index.jsx -v -t babelify -o public/bundle.js",
    "dev": "npm run watch & live-server --wait=1000 public",
    "start": "node ."
  }
}
